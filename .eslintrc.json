{
  "globals": {
    "React": true,
    "react": true
  },
  "extends": ["plugin:import/recommended", "plugin:import/typescript", "react-app", "prettier"],
  "plugins": ["import", "prettier"],
  "rules": {
    "no-unused-vars": "off",
    "@typescript-eslint/no-unused-vars": "off",
    "import/no-named-as-default-member": "off"
  }
}
